<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SharedFlow name="default">
    <Step>
        <Name>AM-SetAPIVersionTo1-0-2</Name>
        <Condition>(proxy.basepath != "/consents/v1") and (proxy.basepath != "/accounts/v1/accounts")</Condition>
    </Step>
    <Step>
        <Name>AM-SetAPIVersionTo1-0-3</Name>
        <Condition>(proxy.basepath = "/consents/v1") or (proxy.basepath = "/accounts/v1/accounts")</Condition>
    </Step>
    <Step>
        <Name>AM-Set-Content-Type-And-Version-Headers</Name>
    </Step>
    <Step>
        <FaultRules/>
        <Name>FC-AddFAPIInteractionId</Name>
    </Step>
    <Step>
        <FaultRules/>
        <Name>AM-Add-CORS-Headers</Name>
    </Step>
    <Step>
        <Name>AM-SetRequestDateTime</Name>
    </Step>
    <Step>
        <Name>JS-Add-Links-Meta-To-Response</Name>
    </Step>
</SharedFlow>