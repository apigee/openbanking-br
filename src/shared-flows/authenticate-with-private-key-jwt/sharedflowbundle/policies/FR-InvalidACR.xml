<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="FR-InvalidACR">
    <DisplayName>FR-InvalidACR</DisplayName>
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="application/json">{
  "errors": [
    {
      "code": "401",
      "title": "Unauthorized",
      "detail": "Invalid acr claim: {PKJwtAdditionalClaims.acr}"
    }
  ]
}
</Payload>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>